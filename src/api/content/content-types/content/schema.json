{
  "kind": "collectionType",
  "collectionName": "contents",
  "info": {
    "singularName": "content",
    "pluralName": "contents",
    "displayName": "content",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "animation",
        "activity",
        "audio",
        "book",
        "game",
        "website",
        "video"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "uuid": {
      "type": "uid"
    },
    "meta": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "suitableFor": {
      "type": "enumeration",
      "enum": [
        "boy",
        "girl",
        "both"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "attachments": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "source": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "sourceUrl": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "ageCategory": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "srcFile": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "editions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::edition.edition",
      "mappedBy": "content"
    },
    "movies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::movie.movie",
      "mappedBy": "content"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "maxAge": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer"
    },
    "tasks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::task.task",
      "mappedBy": "content"
    },
    "content_tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::content-tag.content-tag",
      "mappedBy": "content"
    }
  }
}
